<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import com.jfoenix.controls.JFXButton?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="edu.icet.controller.customer.CustomerControllerDash"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Rectangle arcWidth="5.0" fill="DODGERBLUE" height="78.0" layoutX="7.0" layoutY="7.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="586.0" />
        <Label layoutX="213.0" layoutY="19.0" text="Customer" textFill="WHITE">
            <font>
                <Font size="36.0" />
            </font>
        </Label>
        <JFXButton fx:id="addCustomerBtn" layoutX="242.0" layoutY="123.0" onAction="#addCustomerBtnOnAction" prefHeight="34.0" prefWidth="115.0" style="-fx-background-color: blue;" text="Add Customer" textFill="WHITE">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </JFXButton>
        <JFXButton fx:id="viewCustomerBtn" layoutX="242.0" layoutY="183.0" onAction="#viewCustomerBtnOnAction" prefHeight="34.0" prefWidth="115.0" style="-fx-background-color: blue;" text="View Customer" textFill="WHITE">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </JFXButton>
        <JFXButton fx:id="searchCustomerBtn" layoutX="242.0" layoutY="244.0" onAction="#searchCustomerBtnOnAction" prefHeight="34.0" prefWidth="115.0" style="-fx-background-color: blue;" text="Search Customer" textFill="WHITE">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </JFXButton>
        <JFXButton fx:id="deleteCustomerBtn" layoutX="242.0" layoutY="304.0" onAction="#deleteCustomerBtnOnAction" prefHeight="34.0" prefWidth="115.0" style="-fx-background-color: red;" text="Delete Customer" textFill="WHITE">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </JFXButton>
    </children>
</AnchorPane>
